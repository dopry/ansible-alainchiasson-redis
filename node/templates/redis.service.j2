# NOTE: service name is used by redis-shutdown to derive config file name

[Unit]
Description=Redis persistent key-value database - port {{ redis_port }}
After=network.target

[Service]
ExecStart=/usr/bin/redis-server {{ redis_conf_dir }}/redis_{{ redis_port }}.conf --daemonize no
ExecStop=/usr/bin/redis-shutdown redis_{{ redis_port }}
User=redis
Group=redis
Restart=always
RestartSec=180

[Install]
WantedBy=multi-user.target
Requires=disable-thp.service